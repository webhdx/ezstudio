            {% include '@ezdesign/zone/zone.html.twig' with {'blocks': zones[0].blocks} %}
        {% endif %}
    </div>
    <div class="landing-page__zone landing-page__zone--{{ zones[1].id }} landing-page__zone--right col-9" data-zone="{{ zones[1].id }}">
